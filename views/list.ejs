<%- include("header") -%>
    <div class="box" id="heading">
    <h1> <%= listTitle %></h1>
</div>
<div class="box">
       
        <% for(var i=0;i<newItems.length;i++){ %>
            <div class="item"> 
                <input type="checkbox">
                <p><%= newItems[i] %></p>
            </div>
       
        <%} %>
        <form  class="item" method="post" action="/">
            <input type="text" name="newItem" autocomplete="off" placeholder="New Item">
            <button type="submit" class="btn" name="list" value="<%= listTitle%>">+</button>
        </form>
    </div>
    <%- include("footer") -%>
